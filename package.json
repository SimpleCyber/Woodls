{
  "name": "woodls",
  "version": "1.0.0",
  "description": "A modern XO (Tic Tac Toe) desktop game built using Electron.js",
  "main": "index.js",
  "author": "Satyam Yadav",
  "license": "ISC",
  "type": "commonjs",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^39.2.0",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9"
  },
  "build": {
    "appId": "com.satyam.xogame",
    "productName": "XO Game",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/image.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@jitsi/robotjs": "^0.6.18",
    "@xenova/transformers": "^2.17.2",
    "active-win": "^8.2.1",
    "axios": "^1.13.2",
    "mic": "^2.1.2",
    "mic-recorder-to-mp3": "^2.2.2",
    "node-ahk": "^1.0.7",
    "node-global-key-listener": "^0.3.0",
    "node-piper": "^1.0.0",
    "node-whisper": "^2024.11.13",
    "nodejs-whisper": "^0.2.9",
    "openai": "^6.9.0",
    "robotjs": "^0.6.0",
    "wav": "^1.0.2"
  }
}
